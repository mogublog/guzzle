{
    "name": "mogublog/guzzle",
    "description": "Guzzle is a PHP HTTP client library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "GuzzleHttp\\": "src/guzzle/",
            "GuzzleHttp\\Promise\\": "src/promises/",
            "GuzzleHttp\\Psr7\\": "src/psr7/",
            "Psr\\Http\\Client\\": "src/http-client/",
            "Psr\\Http\\Message\\": "src/http-message/"
        },
        "files": [
            "src/guzzle/functions_include.php",
            "src/promises/functions_include.php",
            "src/psr7/functions_include.php"
        ]
    }
}
